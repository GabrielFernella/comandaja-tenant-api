# Postgres Superuser (for db container)
POSTGRES_USER=prod_user
POSTGRES_PASSWORD=CHANGE_ME_IN_PRODUCTION
POSTGRES_DB=tenants_db_prod

# Services URL
AUTH_SERVICE_URL=http://auth-service-api:3001
