# Production Environment Variables

NODE_ENV=production

# App Port
PORT=3005

# Database
DB_HOST=db-tenants
DB_PORT=5432
DB_USER=prod_user
DB_PASS=CHANGE_ME_IN_PRODUCTION
DB_NAME=tenants_db_prod
DB_CONTAINER_NAME=tenants-db-instance-prod
